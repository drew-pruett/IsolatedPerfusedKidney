	<solutionint> 30.0 </solutionint>
	<displayint> 30.0 </displayint>
</gofor>

<dumppoint><filename> Solutions\Viskoper\Control80.csv </filename><append/></dumppoint>

<setvalue>
  <var> External.Isoproteronol </var>
